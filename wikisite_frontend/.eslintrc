{
  "extends": "airbnb",
  "rules": {
    "react/prefer-stateless-function": {
      "enabled": 0
    },
    "react/jsx-one-expression-per-line": {
      "enabled": 0
    },
    "react/prop-types": {
      "enabled": 0
    },
    "import/prefer-default-export": {
      "enabled": 0
    }
  },
  "env": {
    "jest": true,
    "browser": true
  }
}